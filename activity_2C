#include <stdio.h>

int main() {
int a = 3, b = 30;

printf("%d + %d = %d \n", a, b, a + b);
printf("%d - %d = %d \n", a, b, a - b);
printf("%d * %d = %d \n", a, b, a * b);
printf("%d / %d = %d \n", a, b, a / b);
printf("%d %% %d = %d \n", a, b, a % b);

printf("%d == %d : %d\n", a, b, a == b);
printf("%d != %d : %d\n", a, b, a != b);
printf("%d > %d : %d\n", a, b, a > b);
printf("%d < %d : %d\n", a, b, a < b);
printf("%d >= %d : %d\n", a, b, a >= b);
printf("%d <= %d : %d\n", a, b, a <= b);

printf("(%d && %d) = %d\n", a, b, a && b);
printf("(%d || %d) = %d\n", a, b, a || b);
printf("!%d = %d\n", a, !a);
printf("!%d = %d\n", b, !b);

return 0;
}
